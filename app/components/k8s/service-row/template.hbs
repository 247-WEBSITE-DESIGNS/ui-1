<td>
  {{badge-state model=model}}
</td>
<td class="force-wrap">
  {{model.displayName}} ({{model.id}})
</td>
<td>
  {{default-str model.spec.type default="Cluster IP"}}
</td>
<td class="force-wrap">
  {{#each model.spec.ports as |port|}}
    <span tooltip="{{port.name}}">{{port.port}}:{{port.targetPort}}/{{lower-case port.protocol}}
    </span>
  {{else}}
    <span class="text-muted">None</span>
  {{/each}}
</td>
<td class="force-wrap">
  {{#each model.selectorsAsArray as |sel|}}
    <span class="badge badge-default">{{sel.label}}={{sel.value}}</span>
  {{else}}
    <span class="text-muted">None</span>
  {{/each}}
</td>
<td class="actions">
  {{action-menu model=model}}
</td>
